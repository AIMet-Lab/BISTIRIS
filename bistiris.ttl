@prefix : <https://aimet-lab.github.io/BISTIRIS#> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .
@prefix bst: <https://aimet-lab.github.io/BISTIRIS/> .
@prefix clo: <https://w3id.org/arco/ontology/clothing-description/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix a-dd: <https://w3id.org/arco/ontology/denotative-description/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix doco: <http://purl.org/spar/doco/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix yago: <http://yago-knowledge.org/resource/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix a-lite: <https://w3id.org/arco/ontology/arco-lite/> .
@prefix arkivo: <http://purl.org/arkivo/ontology#> .
@prefix schema: <https://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <https://aimet-lab.github.io/BISTIRIS#> .

<https://aimet-lab.github.io/BISTIRIS#> rdf:type owl:Ontology ;
                                         owl:versionIRI bst:bistiris.owl ;
                                         owl:versionInfo "2.0"^^xsd:float ;
                                         dcterms:creator "G. Corona, D. Guidotti, L. Pandolfo, L. Pulina" ;
                                         dcterms:description "BISTIRIS is an ontology for analytically describing Sardinian traditional costumes."@en ;
                                         dcterms:issued "2025.0"^^xsd:float ;
                                         rdfs:label "BISTIRIS ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dcterms:issued rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/11/swrl#isRuleEnabled
swrl:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/ontology/neighboringMunicipality
dbo:neighboringMunicipality rdf:type owl:ObjectProperty ,
                                     owl:SymmetricProperty ;
                            rdfs:domain dbo:Place ;
                            rdfs:range dbo:Place ;
                            rdfs:isDefinedBy "http://dbpedia.org/ontology/" .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Garment
                                           clo:Costume
                                         )
                           ] ;
               rdfs:range :Source ;
               rdfs:isDefinedBy "http://purl.org/dc/terms/" .


###  http://www.essepuntato.it/2008/12/pattern#isContainedBy
po:isContainedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain doco:Figure ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( bibo:Book
                                            bibo:Periodical
                                          )
                            ] ;
                 rdfs:isDefinedBy "http://www.essepuntato.it/2008/12/pattern#" .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:SymmetricProperty ,
                               owl:TransitiveProperty ,
                               owl:ReflexiveProperty .


###  https://aimet-lab.github.io/BISTIRIS#colourThroughTransparency
:colourThroughTransparency rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :F-Costume
                                                       :F-Garment
                                                     )
                                       ] ;
                           rdfs:range dbo:Colour .


###  https://aimet-lab.github.io/BISTIRIS#coveredBy
:coveredBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :covers ;
           rdfs:domain :Garment ;
           rdfs:range :Garment .


###  https://aimet-lab.github.io/BISTIRIS#coveredByTransparency
:coveredByTransparency rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       owl:inverseOf :coversWithTransparency ;
                       rdfs:domain :F-Garment ;
                       rdfs:range [ rdf:type owl:Class ;
                                    owl:unionOf ( :Curly_apron
                                                  :Shoulder_veil
                                                  :Veil
                                                )
                                  ] .


###  https://aimet-lab.github.io/BISTIRIS#coveredColour
:coveredColour rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain clo:Costume ;
               rdfs:range dbo:Colour .


###  https://aimet-lab.github.io/BISTIRIS#covers
:covers rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain :Garment ;
        rdfs:range :Garment .


###  https://aimet-lab.github.io/BISTIRIS#coversWithTransparency
:coversWithTransparency rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Curly_apron
                                                    :Shoulder_veil
                                                    :Veil
                                                  )
                                    ] ;
                        rdfs:range :F-Garment .


###  https://aimet-lab.github.io/BISTIRIS#sewn_togheter
:sewn_togheter rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :sewn_togheter ;
               rdf:type owl:SymmetricProperty ;
               rdfs:domain :Garment ;
               rdfs:range :Garment .


###  https://schema.org/fromLocation
schema:fromLocation rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Garment
                                                clo:Costume
                                              )
                                ] ;
                    rdfs:range dbo:Place ;
                    rdfs:comment "Traditional of" ;
                    rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/hasPart
schema:hasPart rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf schema:isPartOf ;
               rdf:type owl:InverseFunctionalProperty ;
               rdfs:domain clo:Costume ;
               rdfs:range :Garment ;
               rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/isPartOf
schema:isPartOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Garment ;
                rdfs:range clo:Costume ;
                rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/location
schema:location rdf:type owl:ObjectProperty ;
                rdfs:domain dbo:Place ;
                rdfs:range yago:Provinces_of_Italy ;
                rdfs:isDefinedBy "https://schema.org/" .


###  https://w3id.org/arco/ontology/arco-lite/hasColour
a-lite:hasColour rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Garment
                                             clo:Costume
                                           )
                             ] ;
                 rdfs:range dbo:Colour ;
                 rdfs:isDefinedBy "https://w3id.org/arco/ontology/arco-lite/" .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/elevation
dbo:elevation rdf:type owl:DatatypeProperty ;
              rdfs:domain dbo:Place ;
              rdfs:range xsd:float ;
              rdfs:isDefinedBy "http://dbpedia.org/ontology/" .


###  http://dbpedia.org/property/name
dbp:name rdf:type owl:DatatypeProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range xsd:string ;
         rdfs:isDefinedBy "http://dbpedia.org/property/" .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( bibo:Book
                                          bibo:Periodical
                                        )
                          ] ;
              rdfs:range rdfs:Literal ;
              rdfs:isDefinedBy "http://purl.org/dc/terms/" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
geo:lat rdf:type owl:DatatypeProperty ;
        rdfs:domain dbo:Place ;
        rdfs:range xsd:float ;
        rdfs:isDefinedBy "http://www.w3.org/2003/01/geo/wgs84_pos#" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
geo:long rdf:type owl:DatatypeProperty ;
         rdfs:domain dbo:Place ;
         rdfs:range xsd:float ;
         rdfs:isDefinedBy "http://www.w3.org/2003/01/geo/wgs84_pos#" .


###  https://aimet-lab.github.io/BISTIRIS#BW
:BW rdf:type owl:DatatypeProperty ;
    rdfs:domain :Source ;
    rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#bow
:bow rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf :technical_details ;
     rdfs:domain :F-Garment ;
     rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#checkered_panel
:checkered_panel rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :skirt_panel ;
                 rdfs:domain :Long_skirt ;
                 rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#curly
:curly rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :technical_details ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Curly_apron
                                   :Giacchino
                                   :Long_skirt
                                   :Skirt_trousers
                                 )
                   ] ;
       rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#drawing
:drawing rdf:type owl:DatatypeProperty ;
         rdfs:domain :Source ;
         rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#embroideries
:embroideries rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :technical_details ;
              rdfs:domain :Garment ;
              rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#flaps
:flaps rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :technical_details ;
       rdfs:domain :F-Jacket ;
       rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#floral_embroideries
:floral_embroideries rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :embroideries ;
                     rdfs:domain :F-Garment ;
                     rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#fringes
:fringes rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :technical_details ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Head_shawl
                                     :Shawl
                                   )
                     ] ;
         rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#intended_use
:intended_use rdf:type owl:DatatypeProperty ;
              rdfs:domain clo:Costume ;
              rdfs:range xsd:string .


###  https://aimet-lab.github.io/BISTIRIS#lace
:lace rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :technical_details ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Curly_apron
                                  :Giacchino
                                )
                  ] ;
      rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#màsculas
:màsculas rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :technical_details ;
          rdfs:domain :Gheroni_skirt ;
          rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#open_slevees
:open_slevees rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :technical_details ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :F-Giubbetto
                                          :M-Giubbetto
                                        )
                          ] ;
              rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#panel_buttons
:panel_buttons rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :skirt_panel ;
               rdfs:domain :Long_skirt ;
               rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#pleating
:pleating rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :technical_details ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Apron
                                      :F-Skirt
                                    )
                      ] ;
          rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#points
:points rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :technical_details ;
        rdfs:domain :F-Bodice ;
        rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#skirt_panel
:skirt_panel rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :technical_details ;
             rdfs:domain :Long_skirt ;
             rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#sleeve_buttonholes
:sleeve_buttonholes rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :technical_details ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :F-Jacket
                                                :M-Jacket
                                              )
                                ] ;
                    rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#sleeve_buttons
:sleeve_buttons rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :technical_details ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :F-Jacket
                                            :M-Jacket
                                          )
                            ] ;
                rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#sleeve_ribbon
:sleeve_ribbon rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :technical_details ;
               rdfs:domain :F-Jacket ;
               rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#striped_panel
:striped_panel rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :skirt_panel ;
               rdfs:domain :Long_skirt ;
               rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#tassels
:tassels rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :technical_details ;
         rdfs:domain :F-Headwear ;
         rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#technical_details
:technical_details rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Garment .


###  https://aimet-lab.github.io/BISTIRIS#wrapping
:wrapping rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :technical_details ;
          rdfs:domain :Panel_apron ;
          rdfs:range xsd:boolean .


###  https://schema.org/author
schema:author rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( bibo:Book
                                          bibo:Periodical
                                        )
                          ] ;
              rdfs:range rdfs:Literal ;
              rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/dateCreated
schema:dateCreated rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Garment
                                               clo:Costume
                                             )
                               ] ;
                   rdfs:range rdfs:Literal ;
                   rdfs:comment "Extended Date/Time Format (EDTF)." ;
                   rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/datePublished
schema:datePublished rdf:type owl:DatatypeProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( bibo:Book
                                                 bibo:Periodical
                                               )
                                 ] ;
                     rdfs:range rdfs:Literal ;
                     rdfs:isDefinedBy "https://schema.org/" .


###  https://schema.org/publisher
schema:publisher rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Source ;
                 rdfs:range rdfs:Literal ;
                 rdfs:isDefinedBy "https://schema.org/" .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Colour
dbo:Colour rdf:type owl:Class ;
           rdfs:subClassOf a-dd:TechnicalCharacteristic ;
           rdfs:isDefinedBy "http://dbpedia.org/ontology/" .


###  http://dbpedia.org/ontology/Place
dbo:Place rdf:type owl:Class ;
          rdfs:isDefinedBy "http://dbpedia.org/ontology/" .


###  http://purl.org/arkivo/ontology#GlamThing
arkivo:GlamThing rdf:type owl:Class ;
                 rdfs:subClassOf :Source ;
                 rdfs:isDefinedBy "http://purl.org/arkivo/ontology#" .


###  http://purl.org/ontology/bibo/Book
bibo:Book rdf:type owl:Class ;
          rdfs:subClassOf :Source ;
          rdfs:isDefinedBy "http://purl.org/ontology/bibo/" .


###  http://purl.org/ontology/bibo/Periodical
bibo:Periodical rdf:type owl:Class ;
                rdfs:subClassOf :Source ;
                rdfs:isDefinedBy "http://purl.org/ontology/bibo/" .


###  http://purl.org/spar/doco/Figure
doco:Figure rdf:type owl:Class ;
            rdfs:subClassOf :Source ;
            rdfs:isDefinedBy "http://purl.org/spar/doco/" .


###  http://yago-knowledge.org/resource/Provinces_of_Italy
yago:Provinces_of_Italy rdf:type owl:Class ;
                        rdfs:subClassOf owl:Thing ;
                        rdfs:isDefinedBy "http://yago-knowledge.org/resource/" .


###  https://aimet-lab.github.io/BISTIRIS#Apron
:Apron rdf:type owl:Class ;
       rdfs:subClassOf :F-Garment ;
       owl:disjointUnionOf ( :Curly_apron
                             :Fan-shaped_apron
                             :Panel_apron
                           ) .


###  https://aimet-lab.github.io/BISTIRIS#Bell-shaped_trousers
:Bell-shaped_trousers rdf:type owl:Class ;
                      rdfs:subClassOf :M-Skirt ,
                                      :Trousers .


###  https://aimet-lab.github.io/BISTIRIS#Belt
:Belt rdf:type owl:Class ;
      rdfs:subClassOf :M-Garment .


###  https://aimet-lab.github.io/BISTIRIS#Berritta
:Berritta rdf:type owl:Class ;
          rdfs:subClassOf :M-Headwear .


###  https://aimet-lab.github.io/BISTIRIS#Black_category
:Black_category rdf:type owl:Class ;
                rdfs:subClassOf dbo:Colour .


###  https://aimet-lab.github.io/BISTIRIS#Blue_category
:Blue_category rdf:type owl:Class ;
               rdfs:subClassOf dbo:Colour .


###  https://aimet-lab.github.io/BISTIRIS#Bolero
:Bolero rdf:type owl:Class ;
        rdfs:subClassOf :F-Jacket .


###  https://aimet-lab.github.io/BISTIRIS#Brimmed_hat
:Brimmed_hat rdf:type owl:Class ;
             rdfs:subClassOf :M-Headwear .


###  https://aimet-lab.github.io/BISTIRIS#Brown_category
:Brown_category rdf:type owl:Class ;
                rdfs:subClassOf dbo:Colour .


###  https://aimet-lab.github.io/BISTIRIS#Corset_belt
:Corset_belt rdf:type owl:Class ;
             rdfs:subClassOf :F-Bodice .


###  https://aimet-lab.github.io/BISTIRIS#Curly_apron
:Curly_apron rdf:type owl:Class ;
             rdfs:subClassOf :Apron ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :curly ;
                               owl:hasValue "true"^^xsd:boolean
                             ] .


###  https://aimet-lab.github.io/BISTIRIS#DVV
:DVV rdf:type owl:Class ;
     rdfs:subClassOf arkivo:GlamThing ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty schema:publisher ;
                       owl:hasValue "Istituto Superiore Regionale Etnografico"
                     ] .


###  https://aimet-lab.github.io/BISTIRIS#F-Bodice
:F-Bodice rdf:type owl:Class ;
          rdfs:subClassOf :F-Garment ;
          owl:disjointUnionOf ( :Corset_belt
                                :F-Rigid_bodice
                                :F-Soft_bodice
                              ) .


###  https://aimet-lab.github.io/BISTIRIS#F-Cap
:F-Cap rdf:type owl:Class ;
       rdfs:subClassOf :F-Headwear .


###  https://aimet-lab.github.io/BISTIRIS#F-Costume
:F-Costume rdf:type owl:Class ;
           rdfs:subClassOf clo:Costume ,
                           [ owl:intersectionOf ( clo:Costume
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:allValuesFrom :F-Garment
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Headwear
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:maxQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Headwear
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Skirt
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Skirt
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :Apron
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Bodice
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Jacket
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Shirt
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty schema:hasPart ;
                                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :F-Overgarment
                                                  ]
                                                ) ;
                             rdf:type owl:Class
