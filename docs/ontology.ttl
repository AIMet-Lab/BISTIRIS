@prefix : <https://aimet-lab.github.io/BISTIRIS#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://aimet-lab.github.io/BISTIRIS#> .

<https://aimet-lab.github.io/BISTIRIS#> rdf:type owl:Ontology ;
                                         owl:versionIRI <https://aimet-lab.github.io/BISTIRIS/bistiris.owl> ;
                                         <http://purl.org/dc/terms/creator> "G. Corona, D. Guidotti, L. Pandolfo, L. Pulina" ;
                                         <http://purl.org/dc/terms/description> "BISTIRIS is an ontology for analytically describing Sardinian traditional costumes."@en ;
                                         <http://purl.org/dc/terms/issued> "2025.0"^^xsd:float ;
                                         rdfs:label "BISTIRIS ontology"@en ;
                                         owl:versionInfo "2.0"^^xsd:float .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/11/swrl#isRuleEnabled
<http://www.w3.org/2003/11/swrl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdf:type owl:FunctionalProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( :Garment
                                                              <https://w3id.org/arco/ontology/clothing-description/Costume>
                                                            )
                                              ] ;
                                  rdfs:range :Source .


###  http://www.essepuntato.it/2008/12/pattern#isContainedBy
<http://www.essepuntato.it/2008/12/pattern#isContainedBy> rdf:type owl:ObjectProperty ;
                                                          rdfs:domain <http://purl.org/spar/doco/Figure> ;
                                                          rdfs:range [ rdf:type owl:Class ;
                                                                       owl:unionOf ( <http://purl.org/ontology/bibo/Book>
                                                                                     <http://purl.org/ontology/bibo/Periodical>
                                                                                   )
                                                                     ] .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:ObjectProperty ,
                               owl:SymmetricProperty ,
                               owl:TransitiveProperty ,
                               owl:ReflexiveProperty .


###  https://aimet-lab.github.io/BISTIRIS#colourThroughTransparency
:colourThroughTransparency rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :F-Costume
                                                       :F-Garment
                                                     )
                                       ] ;
                           rdfs:range <https://dbpedia.org/ontology/Colour> .


###  https://aimet-lab.github.io/BISTIRIS#coveredBy
:coveredBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :covers ;
           rdfs:domain :Garment ;
           rdfs:range :Garment .


###  https://aimet-lab.github.io/BISTIRIS#coveredByTransparency
:coveredByTransparency rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       owl:inverseOf :coversWithTransparency ;
                       rdfs:domain :F-Garment ;
                       rdfs:range [ rdf:type owl:Class ;
                                    owl:unionOf ( :Curly_apron
                                                  :Shoulder_veil
                                                  :Veil
                                                )
                                  ] .


###  https://aimet-lab.github.io/BISTIRIS#coveredColour
:coveredColour rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain <https://w3id.org/arco/ontology/clothing-description/Costume> ;
               rdfs:range <https://dbpedia.org/ontology/Colour> .


###  https://aimet-lab.github.io/BISTIRIS#covers
:covers rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain :Garment ;
        rdfs:range :Garment .


###  https://aimet-lab.github.io/BISTIRIS#coversWithTransparency
:coversWithTransparency rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Curly_apron
                                                    :Shoulder_veil
                                                    :Veil
                                                  )
                                    ] ;
                        rdfs:range :F-Garment .


###  https://aimet-lab.github.io/BISTIRIS#sewn_togheter
:sewn_togheter rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :sewn_togheter ;
               rdf:type owl:SymmetricProperty ;
               rdfs:domain :Garment ;
               rdfs:range :Garment .


###  https://dbpedia.org/ontology/neighboringMunicipality
<https://dbpedia.org/ontology/neighboringMunicipality> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <http://yago-knowledge.org/resource/Comune> ;
                                                       rdfs:range <http://yago-knowledge.org/resource/Comune> .


###  https://schema.org/fromLocation
<https://schema.org/fromLocation> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdf:type owl:FunctionalProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( :Garment
                                                              <https://w3id.org/arco/ontology/clothing-description/Costume>
                                                            )
                                              ] ;
                                  rdfs:range <https://schema.org/Place> ;
                                  rdfs:comment "Traditional of" .


###  https://schema.org/hasPart
<https://schema.org/hasPart> rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             owl:inverseOf <https://schema.org/isPartOf> ;
                             rdf:type owl:InverseFunctionalProperty ;
                             rdfs:domain <https://w3id.org/arco/ontology/clothing-description/Costume> ;
                             rdfs:range :Garment .


###  https://schema.org/isPartOf
<https://schema.org/isPartOf> rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdf:type owl:FunctionalProperty ;
                              rdfs:domain :Garment ;
                              rdfs:range <https://w3id.org/arco/ontology/clothing-description/Costume> .


###  https://schema.org/location
<https://schema.org/location> rdf:type owl:ObjectProperty ;
                              rdfs:domain <http://yago-knowledge.org/resource/Comune> ;
                              rdfs:range <http://yago-knowledge.org/resource/Provinces_of_Italy> .


###  https://w3id.org/arco/ontology/arco-lite/hasColour
<https://w3id.org/arco/ontology/arco-lite/hasColour> rdf:type owl:ObjectProperty ;
                                                     rdfs:domain [ rdf:type owl:Class ;
                                                                   owl:unionOf ( :Garment
                                                                                 <https://w3id.org/arco/ontology/clothing-description/Costume>
                                                                               )
                                                                 ] ;
                                                     rdfs:range <https://dbpedia.org/ontology/Colour> .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/elevation
<http://dbpedia.org/ontology/elevation> rdf:type owl:DatatypeProperty ;
                                        rdfs:domain <http://dbpedia.org/ontology/Place> ;
                                        rdfs:range xsd:float .


###  http://dbpedia.org/property/name
<http://dbpedia.org/property/name> rdf:type owl:DatatypeProperty ;
                                   rdfs:domain owl:Thing ;
                                   rdfs:range xsd:string .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain [ rdf:type owl:Class ;
                                               owl:unionOf ( <http://purl.org/ontology/bibo/Book>
                                                             <http://purl.org/ontology/bibo/Periodical>
                                                           )
                                             ] ;
                                 rdfs:range rdfs:Literal .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:DatatypeProperty ;
                                              rdfs:domain <http://dbpedia.org/ontology/Place> ;
                                              rdfs:range xsd:float .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <http://dbpedia.org/ontology/Place> ;
                                               rdfs:range xsd:float .


###  https://aimet-lab.github.io/BISTIRIS#BW
:BW rdf:type owl:DatatypeProperty ;
    rdfs:domain :Source ;
    rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#bow
:bow rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf :technical_details ;
     rdfs:domain :F-Garment ;
     rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#checkered_panel
:checkered_panel rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :skirt_panel ;
                 rdfs:domain :Long_skirt ;
                 rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#curly
:curly rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :technical_details ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Curly_apron
                                   :Giacchino
                                   :Long_skirt
                                   :Skirt_trousers
                                 )
                   ] ;
       rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#drawing
:drawing rdf:type owl:DatatypeProperty ;
         rdfs:domain :Source ;
         rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#embroideries
:embroideries rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :technical_details ;
              rdfs:domain :Garment ;
              rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#flaps
:flaps rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :technical_details ;
       rdfs:domain :F-Jacket ;
       rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#floral_embroideries
:floral_embroideries rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :embroideries ;
                     rdfs:domain :F-Garment ;
                     rdfs:range rdfs:Literal .


###  https://aimet-lab.github.io/BISTIRIS#fringes
:fringes rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :technical_details ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Head_shawl
                                     :Shawl
                                   )
                     ] ;
         rdfs:range xsd:boolean .


###  https://aimet-lab.github.io/BISTIRIS#intended_use
:intended_use rdf:type owl:DatatypeProperty ;
              rdfs:domain <https://w3id.org/arco/ontology/clothing-description/Costume> ;
              rdfs:range xsd:string .


###  https://aimet-lab.github.io/BISTIRIS#lace
:lace rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :technical_details ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Curly_apron
                                  :Giacchino
                                )
                  ] ;
